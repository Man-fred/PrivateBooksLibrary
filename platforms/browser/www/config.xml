<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="DE" id="io.cordova.myapp387c96" version="0.10.2" windows-packageVersion="0.10.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>PrivateBooksLibrary</name>
    <description>Verwalten Sie Ihre private Büchersammlung und eBooks. Das Programm arbeitet ohne Daten im Internet zu speichern, es gibt aber die Möglichkeit zum zentralen Speichern und dann mehrere Geräte zu synchronisieren.</description>
    <author email="dev@bcss.de" href="https://www.bcss.de/dev">Manfred Bielemeier (BCSS Software &amp; Beratung GmbH)</author>
    <vs:template-name>BlankJS</vs:template-name>
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget id="ananan" platformName="windows" version="0.10.2.0" />
    </vs:platformSpecificValues>
    <content src="index.html" />
    <access origin="https://pbl.bcss.de" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <vs:toolsetVersion>6.3.1</vs:toolsetVersion>
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="SplashScreen" value="screen" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
</widget>
