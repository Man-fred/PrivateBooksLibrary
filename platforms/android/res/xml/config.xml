<widget defaultlocale="de-DE" id="io.cordova.myapp387c96" version="0.3.1" windows-packageVersion="0.3.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
