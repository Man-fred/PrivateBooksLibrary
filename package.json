{
  "name": "private-books-library",
  "version": "1.0.0",
  "private": true,
  "author": "Manfred Bielemeier",
  "homepage": "https://github.com/Man-fred",
  "scripts": {
    "assets": "node ../scripts/pack-cli-assets.mjs",
    "build": "npm run clean && npm run assets && tsc",
