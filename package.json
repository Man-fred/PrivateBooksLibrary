    "prepublishOnly": "npm run build",
    "test": "jest -i",
    "watch": "npm run assets && tsc -w"
  },
  "dependencies": {
    "@capacitor/android": "^3.4.0",
    "@capacitor/cli": "^3.4.0",
    "@capacitor/core": "^3.4.0",
    "@capacitor/device": "^1.1.2",
    "@capacitor/ios": "^3.4.0",
    "@capacitor/network": "^1.0.7",
    "cord": "^1.0.0",
    "cordova-connectivity-monitor": "^1.2.2",
    "cordova-plugin": "1.0.0",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-keyboard": "^1.2.0",
    "cordova-plugin-printer": "^0.8.0",
    "cordova-plugin-purchase": "git+https://github.com/Man-fred/cordova-plugin-purchase.git",
    "cordova-plugin-taptic-engine": "^2.1.0",
    "cordova-plugin-vibration": "^3.1.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "phonegap-plugin-barcodescanner": "8.1.0"
  },
  "cordova": {
    "platforms": [
      "windows",
      "ios",
      "browser",
      "android"
    ],
    "plugins": {
      "cordova-plugin-taptic-engine": {},
      "cordova-plugin-camera": {},
      "cordova-plugin-file": {},
      "cordova-plugin-vibration": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-keyboard": {},
      "cordova-plugin-printer": {},
      "cordova-plugin-device": {},
      "phonegap-plugin-barcodescanner": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "cordova-plugin-purchase": {},
      "cordova-plugin-push-notifications": {}
    }
  },
  "devDependencies": {
    "cordova-android": "^10.1.1",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-add-swift-support": "^2.0.2",
    "cordova-plugin-push-notifications": "^1.1.4",
    "cordova-windows": "^7.0.1",
    "typescript": "^4.5.5"
  }
}
